{
  "name": "testwand",
  "description": "Testwand is a CLI and library for automatically generating TypeScript tests using AI.",
  "keywords": [
    "typescript",
    "test",
    "test-generation",
    "cli",
    "ai",
    "automation",
    "jest"
  ],
  "version": "0.0.2",
  "homepage": "https://testwand.dev/",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "testwand": "./dist/cli.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "format": "prettier --write .",
    "check-format": "prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhagmajer/testwand.git"
  },
  "author": "Marcin Hagmajer",
  "license": "MIT",
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/node": "^22.10.5",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "commander": "^13.0.0"
  }
}
